<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ControladorEquipos</servlet-name>
        <servlet-class>biosProduciones.presentacion.servlets.controladores.ControladorEquipos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorInicio</servlet-name>
        <servlet-class>biosProduciones.presentacion.servlets.controladores.ControladorInicio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorSpot</servlet-name>
        <servlet-class>biosProduciones.presentacion.servlets.controladores.ControladorSpot</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorPersonal</servlet-name>
        <servlet-class>biosProduciones.presentacion.servlets.controladores.ControladorPersonal</servlet-class>
        <multipart-config></multipart-config>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControladorSpot</servlet-name>
        <url-pattern>/spot</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorEquipos</servlet-name>
        <url-pattern>/equipos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorInicio</servlet-name>
        <url-pattern>/inicio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorPersonal</servlet-name>
        <url-pattern>/personal</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>inicio</welcome-file>
    </welcome-file-list>
</web-app>
